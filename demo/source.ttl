@prefix : <#> .
@prefix org: <https://www.w3.org/ns/org#> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .

:SolidProject
  skos:prefLabel "Solid Project" ;
  a org:Organization ;
  org:purpose <solid-community-purpose.ttl> ;
  org:hasUnit :Administrators, :Editors, :Creators .

:Administrators
  a org:OrganizationalUnit ;
  skos:prefLabel "Administrators" ;
  org:purpose "Administrators are responsible for administration of tools, systems, and services used for advancing Solid." ;
  org:hasMember :Tim, :Marrelle, :Os, :Justin, :Jackson .

:Editors
  a org:OrganizationalUnit ;
  skos:prefLabel "Editors" ;
  org:purpose "Editors are responsible for maintaining and reviewing suggestions to the Solid specification." ;
  org:hasMember :Sarven, :Ruben, :Justin, :Dmitri, :Kjetil .

:Creators
  a org:OrganizationalUnit ;
  skos:prefLabel "Creators" ;
  org:purpose "Creators are responsible for maintaining and reviewing suggestions to solidproject.org which includes Solid content such as the newsletter This Month in Solid." ;
  org:hasMember :Vincent, :Nicolas, :Jeff .

